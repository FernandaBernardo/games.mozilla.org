{
  "name": "moz-gdc",
  "private": true,
  "description": "Mozilla GDC 2015",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/mozilla/moz-gdc.git"
  },
  "scripts": {
    "start": "NODE_ENVIRONMENT=production node index.js",
    "dev": "NODE_ENVIRONMENT=dev nodemon index.js",
    "prod": "NODE_ENVIRONMENT=production node index.js",
    "gzip": "find public -name '*.css' -o -name '*.eot' -o -name '*.html' -o -name '*.js' -o -name '*.json' -o -name 'html' -o -name '*.js' -o -name '*.json' -o -name '*.svg' -o -name '*.ttf' -o -name '*.webapp' -o -name '*.woff' | xargs gzip --k -9 -f",
    "postinstall": "npm run gzip"
  },
  "main": "./index",
  "version": "0.0.2",
  "dependencies": {
    "basic-auth": "^1.0.0",
    "node-static": "^0.7.6"
  },
  "devDependencies": {
    "nodemon": "^1.3.7"
  }
}
